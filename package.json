{
  "name": "tslint-config-silind",
  "version": "1.0.23",
  "description": "An opinionated TSLint configuration preset for writing scalable, clean and hygienic code",
  "main": "index.js",
  "scripts": {
    "build": "node_modules/typescript/bin/tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Silind/tslint-config-silind.git"
  },
  "keywords": [
    "tslint",
    "typescript",
    "clean code"
  ],
  "author": "Silind Software",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Silind/tslint-config-silind/issues"
  },
  "homepage": "https://github.com/Silind/tslint-config-silind#readme",
  "dependencies": {
    "tslint": "^5.20.1",
    "tsutils": "^3.17.1",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/**/*.test.(ts|tsx)"
    ],
    "collectCoverage": true
  }
}
